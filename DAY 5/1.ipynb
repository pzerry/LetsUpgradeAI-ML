    "# Write a Python program to find the first 20 non-even prime natural numbers.\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(2,21):\n",
    "    if i==2:\n",
    "        print(i)\n",
    "    else:\n",
    "        for j in range(2,i):\n",
    "             if i%j==0:\n",
    "                break\n",
    "        else:\n",
    "            print(i)\n",
    "        \n",
