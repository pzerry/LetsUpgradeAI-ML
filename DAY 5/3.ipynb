   "source": [
    "# Write a Python program to check if the given string is a Palindrome or Anagram or None of them.\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "a=input(\"S1: \")\n",
    "b=input(\"S2: \")\n",
    "if a[::-1]==a:\n",
    "    print(\"Palindrome\")\n",
    "elif sorted(a)==sorted(b):\n",
    "    print(\"Anagram\")\n",
    "else:\n",
    "    print(\"None of them\")"
   ]
  }
